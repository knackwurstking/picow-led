package components

import "picow-led/internal/api"

type PageDevicesData struct {
*BaseData
Devices []*api.Device
}

templ PageDevices(data *PageDevicesData) {
<main>
    for _, d := range data.Devices {
    <p><code>{ d.Server.Addr }</code></p>
    }
    <script src={ data.ServerPathPrefix + "/static/js/api.js" }></script>
    <script src={ data.ServerPathPrefix + "/static/js/page-devices.js" }></script>
</main>
}
